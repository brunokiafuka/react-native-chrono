{
  "name": "react-native-chrono",
  "version": "0.0.1-beta",
  "description": "An awesome calendar",
  "contributors": [],
  "license": "MIT",
  "homepage": "#readme",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "./lib/typescript",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "example": "yarn workspace react-native-chrono-example",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^15.0.0",
    "typescript": "^5.2.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "date-fns": "^3.3.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-reanimated": "^3.7.1",
    "zustand": "^4.5.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-reanimated": "^3.7.1"
  }
}
